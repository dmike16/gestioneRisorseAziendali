<?xml version="1.0" encoding="UTF-8"?>
<web-app xmnls="http://xmlns.jcp.org/xml/ns/javaee" xmnls:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmnls
jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">

	<display-name>Gestione Dati Anagrafici</display-name>

	<servlet>
